{
  "version": 2,
  "cleanUrls": true,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/cron.js",  "use": "@vercel/node" },

    { "src": "index.html", "use": "@vercel/static" },
    { "src": "reset.html", "use": "@vercel/static" },
    { "src": "forgot.html", "use": "@vercel/static" },
    { "src": "REGISTER.HTML", "use": "@vercel/static" },
    { "src": "HOMEนศทตกลทบ.html", "use": "@vercel/static" },
    { "src": "calendar.html", "use": "@vercel/static" },
    { "src": "event.html", "use": "@vercel/static" },
    { "src": "dist/**", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "headers": [
    { "source": "/:all*\\.js",  "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/:all*\\.css", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/:all*\\.png", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/:all*\\.jpg", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/:all*\\.svg", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "no-cache" }] }
  ],
    "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" }
  ],
  "crons": [
    { "path": "/api/cron", "schedule": "0 0 * * *" }
  ]
}